{% extends 'base.html.twig' %}

{% block title %}Weather{% endblock %}

{% block body %}
    <h1>Weather</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ weather.id }}</td>
            </tr>
            <tr>
                <th>Data_time</th>
                <td>{{ weather.dataTime }}</td>
            </tr>
            <tr>
                <th>Weather_code</th>
                <td>{{ weather.weatherCode }}</td>
            </tr>
            <tr>
                <th>Weather_name</th>
                <td>{{ weather.weatherName }}</td>
            </tr>
            <tr>
                <th>Temperature_min</th>
                <td>{{ weather.temperatureMin }}</td>
            </tr>
            <tr>
                <th>Temperature_max</th>
                <td>{{ weather.temperatureMax }}</td>
            </tr>
            <tr>
                <th>Rain_percentage</th>
                <td>{{ weather.rainPercentage }}</td>
            </tr>
            <tr>
                <th>Pinpoint_code</th>
                <td>{{ weather.pinpointCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('weather_index') }}">back to list</a>

    <a href="{{ path('weather_edit', {'id': weather.id}) }}">edit</a>

    {{ include('weather/_delete_form.html.twig') }}
{% endblock %}
